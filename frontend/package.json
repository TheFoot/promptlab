{
  "name": "promptlab-frontend",
  "private": true,
  "version": "0.9.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:quiet": "SASS_QUIET=1 NODE_OPTIONS=--no-warnings vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "format": "prettier --write .",
    "sass-migrate": "sass-migrator --load-path=./src/styles module **/*.scss",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "highlight.js": "^11.11.1",
    "marked": "^15.0.11",
    "pinia": "^3.0.2",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.27.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "sass-migrator": "^2.3.2",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
